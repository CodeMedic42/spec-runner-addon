{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false,
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true,
      "jsx": true,
      "arrowFunction": true
    }
  },
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jest/globals": true
  }
}